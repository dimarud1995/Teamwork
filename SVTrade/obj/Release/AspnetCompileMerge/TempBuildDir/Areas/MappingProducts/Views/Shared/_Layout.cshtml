<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>

@Styles.Render("~/Areas/MappingProducts/Content/css/animate.css")
@Styles.Render("~/Areas/MappingProducts/Content/css/bootstrap.min.css")
@Styles.Render("~/Areas/MappingProducts/Content/css/main.css")

    <!--[if lt IE 9]>
    <script src="js/html5shiv.js"></script>
    <script src="js/respond.min.js"></script>
    <![endif]-->

</head><!--/head-->
<body>
    <header id="header">
        <!--header-->


        <div class="header-middle">
            <!--header-middle-->
            <div class="container">
                <div class="row">
                    <div class="row">

                        <div class="col-sm-9">
                            <div class="shop-menu pull-right">
                                <ul class="nav navbar-fixed-top header-backgound">
                                    <li><a href="@Url.Action("Index", "Home", new { area = "" })"><i class="fa fa-user"></i>Домашня Сторінка</a> </li>
                                    <li><a href="@Url.Action("Index/" + System.Web.HttpContext.Current.User.Identity.Name, "Personal/ProductToBuys", new { area = "" })"><i class="fa fa-star"></i>Передзамовлення</a></li>
                                    <li><a href="@Url.Action("Index/"+System.Web.HttpContext.Current.User.Identity.Name, "Personal/Orders", new { area = "" })"><i class="fa fa-star"></i>Замовлення</a></li>
                                    <li><a href="@Url.Action("Index/"+System.Web.HttpContext.Current.User.Identity.Name, "Personal/Products", new { area = "" })"><i class="fa fa-star"></i>Продукція</a></li>
                                    @*<li><a href=""><i class="fa fa-unlock"></i> Logout</a></li>*@
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div><!--/header-middle-->

    </header><!--/header-->


    @RenderBody()

    <!-- Modal -->
 
    

    @Scripts.Render("~/Areas/MappingProducts/Scripts/js/jquery.js")
    @Scripts.Render("~/Areas/MappingProducts/Scripts/js/bootstrap.min.js")

    @Scripts.Render("~/Areas/MappingProducts/Scripts/js/main.js")
</body>
</html>